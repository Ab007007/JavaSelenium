package com.synechron.typecasting;

public class TypeCastingDemo {

	
	public static void main(String[] args) 
	{
		
		int a = 100;
		
		double d = a;
		
		System.out.println("A " + a);
		
		System.out.println("D " + d);
		
		
		
		//Explict type casting
		
		double d1 = 100.55;
		
		int a1 = (int)d1;
		
	}
}
