package com.synechron.java.inheritance;

public class Dog extends Animal 
{

	void bark() {
		System.out.println("The dog barks...");
	}
	
	@Override
	void eat() {
		System.out.println("Dog eat Pedigree / non-veg / veg");
	}
	
	public static void main(String[] args) {
		
		Dog d = new Dog();
		d.eat();
		d.makesound();
		d.bark();
	}
}
