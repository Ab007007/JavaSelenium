package com.synechron.java.files;

import java.io.File;
import java.io.IOException;

public class CreateFile {

	
	public static void main(String[] args) {
		
		File f = new File("files/newFile.txt");
		try
		{
			if(!f.exists())
			{
				f.createNewFile();
				
			}
		}
		catch(IOException ioex)
		{
			ioex.printStackTrace();
		}
		
	}
}
