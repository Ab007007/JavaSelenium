package com.synechron.java.datatypes;

public class ArraysDemo {

	
	public static void main(String[] args) {
		
		String[] fruits = {"Apple", "Orange", "Banana"}; 
		
		
		int marks[] = new int[6];
		
		marks[0] = 55;
		marks[1] = 50;
		marks[2] = 52;
		marks[3] = 75;
		marks[4] = 35;
		
		System.out.println(fruits[0]);
		
		System.out.println(marks[1]);
	}
}
